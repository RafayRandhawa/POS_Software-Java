<AnchorPane prefHeight="549.0" prefWidth="825.0" style="-fx-background-color: #bbe1ff;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RemoveFromInventory">
    <children>
        <Label alignment="CENTER" layoutX="-1.0" layoutY="49.0" prefHeight="49.0" prefWidth="882.0" text="Enter Item ID of Item to Remove">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#mainMenu" prefHeight="35.0" prefWidth="147.0" style="-fx-background-color: #2086FF;" text="Back to Main Menu" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button layoutX="800.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout" prefHeight="35.0" prefWidth="68.0" style="-fx-background-color: RED;" text="Logout" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
        <TableView fx:id="ItemTable" layoutY="237.0" prefHeight="294.0" prefWidth="882.0">
            <columns>
                <TableColumn fx:id="ItemID" prefWidth="87.0" text="Item ID" />
                <TableColumn fx:id="itemName" prefWidth="147.0" text="Item Name" />
                <TableColumn fx:id="Price" prefWidth="126.0" text="Price" />
                <TableColumn fx:id="Quantity" minWidth="9.0" prefWidth="124.0" text="Quantity" />
                <TableColumn fx:id="ExpiryDate" prefWidth="120.0" text="Expiry Date" />
                <TableColumn fx:id="SupplierName" prefWidth="145.0" text="Supplier Name" />
            </columns>
        </TableView>
        <TextField fx:id="code" layoutX="350.0" layoutY="116.0" prefHeight="30.0" prefWidth="180.0" />
        <Button layoutX="393.0" layoutY="199.0" mnemonicParsing="false" onAction="#Delete" prefHeight="30.0" prefWidth="104.0" style="-fx-background-color: #2086FF;" text="Delete" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
